  "dependencies": {
    "babel-plugin-inline-react-svg": "^1.1.2",
    "concurrently": "^5.3.0",
    "firebase": "^8.2.5",
    "json-server": "^0.16.3",
    "next": "^10.0.3",
    "next-redux-wrapper": "^6.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.11",
    "@types/react": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/redux-logger": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.15 .0",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "sass": "^1.30.0",
    "typescript": "^4.1.2"
  },
  "husky": {
    "hooks": {
